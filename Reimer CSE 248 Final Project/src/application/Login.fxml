<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="288.0" prefWidth="348.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Label fx:id="messageLbl" layoutX="23.0" layoutY="25.0" prefHeight="25.0" prefWidth="235.0" text="Enter your username and password to login" />
      <TextField fx:id="usernameField" layoutX="101.0" layoutY="59.0" />
      <PasswordField fx:id="passwordField" layoutX="101.0" layoutY="106.0" />
      <Button layoutX="101.0" layoutY="144.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Label layoutX="23.0" layoutY="63.0" text="Username:" />
      <Label layoutX="23.0" layoutY="110.0" text="Password:" />
      <Label layoutX="23.0" layoutY="193.0" text="Don't have an account? Create one here" />
      <Button layoutX="103.0" layoutY="233.0" mnemonicParsing="false" onAction="#createNewAccount" text="Create New Account" />
   </children>
</AnchorPane>
