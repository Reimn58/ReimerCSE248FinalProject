<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="548.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DisplayItemsController">
   <children>
      <TableView fx:id="itemTable" layoutY="97.0" onSort="#generateDisplay" prefHeight="452.0" prefWidth="667.0">
         <columns>
            <TableColumn fx:id="itemNameColumn" prefWidth="156.0" text="Item Name" />
            <TableColumn fx:id="itemPriceColumn" prefWidth="160.0" text="Price" />
            <TableColumn fx:id="itemQuantityColumn" prefWidth="97.0" text="Quantity" />
            <TableColumn fx:id="purchaseItemColumn" prefWidth="253.0" />
         </columns></TableView>
      <Button layoutX="576.0" layoutY="23.0" mnemonicParsing="false" onAction="#mainMenu" text="Main Menu" />
      <Button layoutX="576.0" layoutY="57.0" mnemonicParsing="false" onAction="#myCart" prefHeight="25.0" prefWidth="77.0" text="My Cart" />
      <Label fx:id="msgLabel" layoutX="127.0" layoutY="27.0" />
   </children>
</AnchorPane>
